<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="isCircle" format="boolean"/>
        <attr name="round" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="WaterfallFlowLayout">
        <attr name="column" format="integer"/>
        <attr name="horizontalSpace" format="dimension"/>
        <attr name="verticalSpace" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CircleProgressBar">
        <attr name="progressWidth" format="dimension"/>
        <attr name="backgroundColor" format="color"/>
        <attr name="progressColor" format="color"/>
        <attr name="progressTextColor" format="color"/>
        <attr name="progressTextSize" format="dimension"/>
        <attr name="progress" format="integer"/>
        <attr name="startDegree" format="integer"/>
    </declare-styleable>
</resources>